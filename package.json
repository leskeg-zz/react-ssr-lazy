{
  "name": "my-react-webpack-init",
  "version": "1.0.0",
  "description": "React + Webpack (Server rendering, lazy routes and gzip compression)",
  "main": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/leskeg/my-react-webpack-init.git"
  },
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.dev.js",
    "webpack": "webpack",
    "stats": "webpack --config webpack.config.prod.js --json > stats.json",
    "prod": "webpack --config webpack.config.prod.js && webpack --config webpack.config.serv.js && babel src/server.js --out-file dist/server.js --minified --plugins remove-webpack && node dist/server"
  },
